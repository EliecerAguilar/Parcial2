<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal">



    <TextView android:id="@+id/lblTitulo"
        android:layout_centerHorizontal="true"
        android:layout_width="350dp"
        android:layout_height="30dp"
        android:textStyle="bold"
        android:textSize="20sp" />

    <TextView android:id="@+id/lblSubTitulo"

        android:layout_width="200dp"
        android:layout_height="20dp"
        android:textStyle="normal"
        android:textSize="12sp"
        android:layout_toRightOf="@id/imageview2"
        android:layout_below="@+id/lblTitulo"
        />


    <ImageView
        android:id="@+id/imageview"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:src="@drawable/doublecheck"
        tools:visibility="visible"
        android:layout_toRightOf="@+id/lblTitulo"
        android:layout_alignParentRight="true"

        />


    <ImageView
        android:id="@+id/imageview2"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_centerHorizontal="false"
        android:layout_toLeftOf="@+id/lblTitulo"
        android:src="@drawable/iconocon"
        tools:visibility="visible" />

    <TextView
        android:id="@+id/lblNota"

        android:layout_width="60dp"
        android:layout_height="30dp"

        android:layout_toRightOf="@+id/lblSubTitulo"
        android:textSize="20sp"
        android:textStyle="bold"
        android:layout_below="@+id/lblTitulo"/>



</RelativeLayout>